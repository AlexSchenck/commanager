{
	"extends": [
		"tslint-eslint-rules"
	],
	"rules": {
		"brace-style": [
			"error",
			"1tbs"
		],
		"block-spacing": [
			true,
			"always"
		],
		"object-curly-spacing": [
			true,
			"always"
		],
		"ter-indent": [
			true,
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"space-in-parens": [
			true,
			"never"
		],
		"ter-arrow-spacing": [
			true
		],
		"member-ordering": [
			true,
			"variables-before-functions"
		],
		"no-var-requires": true,
		"no-conditional-assignment": true,
		"no-console": [
			true,
			"assert",
			"count",
			"debug",
			"dir",
			"dirxml",
			"error",
			"group",
			"groupCollapsed",
			"groupEnd",
			"info",
			"log",
			"profile",
			"profileEnd",
			"table",
			"time",
			"timeEnd",
			"timeStamp",
			"trace",
			"warn"
		],
		"no-construct": true,
		"no-debugger": true,
		"no-duplicate-key": true,
		"no-duplicate-variable": true,
		"no-unreachable": true,
		"no-unused-expression": true,
		"no-unused-locals": true,
		"no-unused-variable": true,
		"no-var-keyword": true,
		"prefer-const": true,
		"switch-default": true,
		"triple-equals": true,
		"no-trailing-whitespace": true,
		"object-literal-sort-keys": false,
		"class-name": true,
		"comment-format": [
			true,
			"check-space"
		],
		"interface-name": [
			true,
			"always-prefix"
		],
		"jsdoc-format": true,
		"no-consecutive-blank-lines": true,
		"quotemark": [
			true,
			"single",
			"avoid-escape"
		],
		"semicolon": [
			true,
			"always"
		],
		"variable-name": [
			true,
			"ban-keywords",
			"check-format",
			"allow-leading-underscore",
			"allow-pascal-case"
		],
		"trailing-comma": [
			true,
			{
				"multiline": "never",
				"singleline": "never"
			}
		]
	}
}